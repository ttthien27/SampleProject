<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.sampleproject">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SampleProject"
        tools:targetApi="31">
        <activity
            android:name=".view.activity.NavigationExampleActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.RetrofitExampleActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.ContentProviderActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.FragmentExampleActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.ComponentExampleActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.BroadcastExampleActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.ServiceExampleActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.LifeCycleExampleActivity"
            android:exported="false" />
        <activity
            android:name=".view.activity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <nav-graph android:value="@navigation/nav_graph" />
        </activity>
    </application>

</manifest>